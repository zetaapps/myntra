<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Needed for ScrimInsetsFrameLayout to declare custom scrim color -->
    <declare-styleable name="ScrimInsetsView">
        <attr name="insetForegroundCompat" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="ZetaFont">

        <attr name="fontStyle" format="enum">
            <enum name="regular" value="0" />
            <enum name="regularBold" value="1" />
            <enum name="regularItalics" value="2" />
            <enum name="regularLeitura" value="3" />
        </attr>
    </declare-styleable>

</resources>